{
  "rules": {
    "member-access": true,
    "no-reference": true,
    "indent": [
      true,
      "spaces",
      2
    ],
    "typedef": [
      true,
      "call-signature",
      "call-signature",
      "parameter",
      "property-declaration",
      "member-variable-declaration"
    ],
    "semicolon": [
      true,
      "never"
    ],
    "only-arrow-functions": true,
    "curly": true,
    "no-duplicate-variable": true,
    "no-empty": true,
    "no-eval": true,
    "no-null-keyword": true,
    "no-shadowed-variable": true,
    "no-var-keyword": true,
    "switch-default": true,
    "triple-equals": true,
    "use-isnan": true,
    "prefer-const": true,
    "class-name": true,
    "no-angle-bracket-type-assertion": true,
    "no-consecutive-blank-lines": true
  }
}
